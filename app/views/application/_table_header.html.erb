<tr>
  <% titles.each do |title| %>
    <th><%= model.human_attribute_name(title) %></th>
  <% end %>
  <th class="actions">
    <%= link_to [:new, namespace, model.model_name.singular_route_key], title: new_label, class: 'btn btn-new btn-primary' do  %>
      <i class="icon-plus-sign"></i> <%= new_label %>
    <% end %>
  </th>
</tr>
